#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>


int main() {
    char s[11];
    scanf("%s", s);
    int freq[5] = {0};
    for (int i = 0; i < 10; i++) {
        freq[s[i]-'a']++;
    }
    for (int i = 0; i < 5; i++) {
        printf("%d ", freq[i]);
    }
    return 0;
}
