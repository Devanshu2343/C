//pointer variable declaration
#include<stdio.h>
int main()
{

    int x=20;
    printf("%d \n",x);
    printf("%u \n",&x);
    int *p1;
    p1=&x;//p1 containing address of 1
    printf("%u",p1);

    float *name,name1=23.56;
    printf("%f \n",name1);
    name=&name1;
    printf("%u\n",&name1);
    printf("%u\n",name);
}
