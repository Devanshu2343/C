
#include<stdio.h>
main()
{

    int i,j,k;
    int num;
      printf("Enter a number : ");
    scanf("%d",&num);
    for(i=2;i<=num;i++)
    {
        for(j=1;j<=num;j++)
            {
                if(i==j)
                    break;
            printf("*");
        }
        printf("\n");
    }
}
