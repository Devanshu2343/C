#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n,p=1,i;
    scanf("%d",&n);
    int ar[n];
    for(i=0;i<n;i++){
        scanf("%d ",&ar[i]);
        p=p*ar[i];
    }
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    printf("%d",p);
    return 0;
}
